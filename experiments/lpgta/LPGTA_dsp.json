{
  "outputs": [ "trapE", "trapEftp", "ct_corr", "bl", "bl_sig", "A_10", "AoE",
             "tp_max", "tp_0", "tp_10", "tp_50", "tp_80", "tp_90",
             "dcr_raw"],
  "processors":{
    "bl, bl_sig":{
      "function": "mean_stdev",
      "module": "pygama.dsp.processors",
      "args" : ["waveform[0:1000]", "bl", "bl_sig"],
      "prereqs": ["waveform"],
      "unit": ["ADC", "ADC"]
    },
    "wf_blsub":{
      "function": "subtract",
      "module": "numpy",
      "args": ["waveform", "bl", "wf_blsub"],
      "prereqs": ["waveform", "bl"],
      "unit": "ADC"
    },
    "wf_pz": {
      "function": "pole_zero",
      "module": "pygama.dsp.processors",
      "args": ["wf_blsub", "db.pz_const", "wf_pz"],
      "prereqs": ["wf_blsub"],
      "unit": "ADC",
      "defaults": { "db.pz_const":"400*us" }
    },
    "wf_trap": {
      "function": "trap_norm",
      "module": "pygama.dsp.processors",
      "args": ["wf_pz", "10*us", "3*us", "wf_trap"],
      "prereqs": ["wf_pz"],
      "unit": "ADC"
    },
    "wf_atrap": {
      "function": "asymTrapFilter",
      "module": "pygama.dsp.processors",
      "args": ["wf_pz", "0.05*us", "2*us", "3*us", "wf_atrap"],
      "prereqs": ["wf_pz"],
      "unit": "ADC"
    },
    "trapE": {
      "function": "amax",
      "module": "numpy",
      "args": ["wf_trap", 1, "trapE"],
      "kwargs": {"signature":"(n),()->()", "types":["fi->f"]},
      "unit": "ADC",
      "prereqs": ["wf_trap"]
    },
    "trapEftp": {
      "function": "fixed_time_pickoff",
      "module": "pygama.dsp.processors",
      "args": ["wf_trap", "tp_0+13.5*us", "trapEftp"],
      "kwargs": {"signature":"(n),()->()", "types":["fi->f"]},
      "unit": "ADC",
      "prereqs": ["wf_trap", "tp_0"]
    },
    "curr10": {
      "comment": "3rd argument allocates memory for a derivative of type float",
      "function": "avg_current",
      "module": "pygama.dsp.processors",
      "args": ["wf_pz", 10, "curr10(len(wf_pz)-10, f)"],
      "unit": "ADC/sample",
      "prereqs": ["wf_pz"]
    },
    "A_10": {
      "function": "amax",
      "module": "numpy",
      "args": ["curr10", 1, "A_10"],
      "kwargs": {"signature":"(n),()->()", "types":["fi->f"]},
      "unit": "ADC",
      "prereqs": ["curr10"]
    },
    "AoE": {
      "function": "divide",
      "module": "numpy",
      "args":["A_10", "trapE", "AoE"],
      "unit": "1/sample",
      "prereqs": ["A_10", "trapE"]
    },
    "tp_max": {
      "function": "argmax",
      "module": "numpy",
      "args": ["wf_blsub", 1, "tp_max"],
      "kwargs": {"signature":"(n),()->()", "types":["fi->i"]},
      "unit": "ns",
      "prereqs": ["wf_blsub"]
    },
    "tp_0": {
      "function": "time_point_thresh",
      "module": "pygama.dsp.processors",
      "args": ["wf_atrap", 0, "tp_max", "tp_0"],
      "unit": "ns",
      "prereqs": ["wf_atrap", "tp_max"]
     },
    "tp_10": {
      "function": "time_point_frac",
      "module": "pygama.dsp.processors",
      "args": ["wf_blsub", 0.1, "tp_max", "tp_10"],
      "unit": "ns",
      "prereqs": ["wf_blsub", "tp_max"]
    },
    "tp_50": {
      "function": "time_point_frac",
      "module": "pygama.dsp.processors",
      "args": ["wf_blsub", 0.5, "tp_max", "tp_50"],
      "unit": "ns",
      "prereqs": ["wf_blsub", "tp_max"]
    },
    "tp_80": {
      "function": "time_point_frac",
      "module": "pygama.dsp.processors",
      "args": ["wf_blsub", 0.8, "tp_max", "tp_80"],
      "unit": "ns",
      "prereqs": ["wf_blsub", "tp_max"]
    },
    "tp_90": {
      "function": "time_point_frac",
      "module": "pygama.dsp.processors",
      "args": ["wf_blsub", 0.9, "tp_max", "tp_90"],
      "unit": "ns",
      "prereqs": ["wf_blsub", "tp_max"]
    },
    "dcr_raw": {
      "function": "trap_pickoff",
      "module": "pygama.dsp.processors",
      "args": ["wf_pz", 100, 3000, "tp_90+1*us", "dcr_raw"],
      "unit": "ADC",
      "prereqs": ["wf_pz", "tp_90"]
    },
    "ct_corr": {
      "function": "trap_pickoff",
      "module": "pygama.dsp.processors",
      "args":["wf_pz", "4*us", 0, "tp_0", "ct_corr"],
      "unit": "ADC",
      "prereqs": ["wf_pz", "tp_0"]
    }
  }
}
