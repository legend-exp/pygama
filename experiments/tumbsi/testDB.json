{
  "loc_dir":"/Users/kermaidic/Documents/GitHub/pygama/experiments/tumbsi/",
  "raw_dir":"/Users/kermaidic/Documents/GitHub/pygama/experiments/tumbsi/raw/",
  "tier_dir":"/Users/kermaidic/Documents/GitHub/pygama/experiments/tumbsi/tier",
  "digitizer":"FlashCamModel",
  "daq":"FlashCam",
  "t1_prefix":"t1_run",
  "t2_prefix":"t2_run",
  "chunksize":1000,
  "clock":62.5e6,
  "rollover":1,
  "mass_note":"(5.323 g/cm^3) * ((pi * 3.1^2 * 4.6) cm^3) / 1000",
  "det_mass_kg":2,

  "ds":{
    "note":"data sets",
    "0":["000", "2018/09 thorium calibration with BSI ICPC at TUM "]
  },
  "build_options":{
    "conf1":{
      "run_coverage":[0,99999],
      "tier0_options":{
        "daq":"FlashCam",
        "digitizer":"FlashCamWaveformDecoder"
      },
      "tier1_options":{
        "clk"    : 62.5e6,
        "fit_bl" : {"ihi":100, "order":1},
        "blsub"  : {"blest":"daq"},
        "pz"     : {"decay":35},
        "notch"  : {"f_notch":30e6, "Q":1},
        "savgol" : [
            {"wfin":"wf_blsub", "wfout":"wf_savgol", "window":5, "order":2},
            {"wfin":"wf_notch", "wfout":"wf_notsav", "window":5, "order":2}
        ],
        "trap" : [
            {"wfout":"wf_etrap", "wfin":"wf_notch", "rise":3, "flat":1, "decay":35},
            {"wfout":"wf_strap", "wfin":"wf_notch", "rise":1, "flat":1.5, "decay":35},
            {"wfout":"wf_atrap", "wfin":"wf_notch", "rise":0.04, "flat":0.1, "fall":2},
            {"wfout":"wf_ttrap", "wfin":"wf_notch", "rise":1, "flat":0, "fall":1}
        ],
        "current" : [
            {"wfin":"wf_pz", "wfout":"wf_current", "sigma":5}
        ],
        "get_max" : [
            {"wfin":"wf_etrap"}, {"wfin":"wf_strap"}, {"wfin":"wf_atrap"},
            {"wfin":"wf_ttrap"}, {"wfin":"wf_savgol"}, {"wfin":"wf_current"}
        ],
        "timepoint" : {"wfin":"wf_savgol", "pct":[5,10,50,100]},
        "peakdet" : [
            {"delta":0.5, "sigma":5, "ihi":600}
        ],
        "num_peaks" : {},
        "ftp" : {},
        "overflow" : {},
        "dcr" : [
            {"wfin":"wf_savgol"},
            {"wfin":"wf_pz"}
        ],
        "tail_fit" : [
            {"wfin":"wf_savgol", "order":1, "tp_thresh":0.8, "vec":0},
            {"wfin":"wf_pz", "order":1, "tp_thresh":0.8, "vec":0}
        ]
      },
      "unused":{
          "center": {"wfin":"wf_notch"},
          "current_pass2" : {"wfin":"wf_ctr", "wfout":"wf_current_ctr", "sigma":5},
          "test_peakdet" : {"wfin":"wf_current_ctr", "delta":0.1, "sigma":5, "ihi":600, "test":1},
          "psd" : {"nseg":100,"test":1,"note":"this one is slow, use w/ -m mode only"},
          "trim": {"n_pre":100, "n_post":1000},
          "peakdet_test":{"delta":1.5, "sigma":0, "ihi":600},
          "trap_test":{"rise":4, "flat":2, "fall":4, "decay":72}
      }
    }
  }
}
